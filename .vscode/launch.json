{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Debug bst_st",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/out/test_bst",
            "args": [
                "gmtx",
                "10",
                "1000000"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/out",
            "environment": [],
            "externalConsole": false,
            "filterStdout": true,
            "MIMode": "gdb",
            "preLaunchTask": "compile_bst",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}